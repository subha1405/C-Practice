#include<stdio.h>
void main()
{
    int a,i=1,c=0;
    printf("Enter the number");
    scanf("%d",&a);
    for(i=1;i<=a;i++)
    {
        if(a%i==0){
            c+=1;
        }
        else
        {
            c+=0;
        }
    }
    if(c==2)
    {
        printf("%d is a prime number",a);
    }
    else{
        printf("%d is a composite number",a);
    }
}
